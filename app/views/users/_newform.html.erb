<%= form_for @user, html: { autocomplete: :off } do |f| %>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
    <p>
    <%= f.label :password %>
    <%= password_field_tag :password %>
  </p>
    <p>
    <%= f.label :password_confirmation %>
    <%= password_field_tag :password_confirmation %>
  </p>
  <p>
    <%= f.submit :class => "button round" %>
  </p>
<% end %>